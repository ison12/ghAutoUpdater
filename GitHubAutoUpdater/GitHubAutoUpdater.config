<?xml version="1.0" encoding="utf-8" ?>
<root>
  <!-- アップデート対象のアプリケーション名 -->
  <applicationName>CliboDone</applicationName>
  <!-- アプリケーションファイルパス（GitHubAutoUpdater.exeの設置パスからの相対位置） -->
  <applicationFilePath>..\CliboDone.exe</applicationFilePath>
  <!-- アプリケーションファイルのバージョン番号（GitHubAutoUpdater.exeの設置パスからの相対位置） -->
  <applicationVersionFilePath>..\CliboDone.version</applicationVersionFilePath>
  <!-- アプリケーションのアップデートバッチ（最新リリース情報のダウンロードファイル内にある GitHubAutoUpdater.exe の設置パスからの相対位置） -->
  <updateScriptFilePath>Update.bat</updateScriptFilePath>
  <!-- アプリケーションのアップデート時に停止すべきプロセスの一覧 -->
  <updateCheckProcesses>
    <process>CliboDone</process>
  </updateCheckProcesses>
  <!-- GitHub APIの設定情報 -->
  <github>
    <rootUrl>https://api.github.com</rootUrl>
    <!-- 最新リリース情報の取得 -->
    <releasesLatest>
      <!-- 最新リリース情報のURL -->
      <uri>repos/:owner/:repo/releases/latest</uri>
      <!-- 所有者 -->
      <owner>ison12</owner>
      <!-- リポジトリ -->
      <repo>cliboDone</repo>
    </releasesLatest>
  </github>
</root>
